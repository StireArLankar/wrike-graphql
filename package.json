{
  "name": "wrike-graphql",
  "private": true,
  "version": "0.0.0-development",
  "description": "Demo of wrapping REST API by GraphQL",
  "repository": "https://github.com/nodkz/wrike-graphql",
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.0",
    "@graphql-codegen/typescript-graphql-request": "1.15.0",
    "@graphql-codegen/typescript-operations": "1.15.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.6",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "3.9.3"
  },
  "scripts": {
    "dev": "PORT=4444 DEBUG=axios:request ts-node-dev --no-notify --respawn --watch src/schema src/server.ts",
    "eslint": "eslint --ext .ts ./src",
    "watch": "DEBUG=axios:* jest --env node --watch",
    "test": "jest --env node",
    "build": "tsc -p ./tsconfig.build.json",
    "gen-sdk": "ts-node ./bonus/generateSchema && yarn graphql-codegen -c ./bonus/codegen.yml"
  },
  "dependencies": {
    "apollo-server-express": "2.14.1",
    "axios": "^0.19.2",
    "dataloader": "^2.0.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "graphql": "15.0.0",
    "graphql-compose": "7.17.0",
    "graphql-compose-json": "5.3.0",
    "graphql-compose-modules": "1.3.1",
    "graphql-voyager": "1.0.0-rc.31",
    "module-alias": "^2.2.2",
    "nodemon": "2.0.4"
  }
}
